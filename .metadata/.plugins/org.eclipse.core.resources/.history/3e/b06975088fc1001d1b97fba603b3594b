package Soru3;

public class Kertenkele extends Surungen{

	public Kertenkele(String ad, int kilo, int uzunluk, Boolean tehlikeliMi) {
		super(ad, kilo, uzunluk, tehlikeliMi);
		// TODO Auto-generated constructor stub
	}

	@Override
	public void surun() {
		System.out.println(this.getAd() + "isimli kertenkele sürünüyor");
	
		
	}

	@Override
	public void sesCikar() {
		System.out.println("");
		
	}

}
