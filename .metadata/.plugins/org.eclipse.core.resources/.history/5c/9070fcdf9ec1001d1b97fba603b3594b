package Soru2;

public class Main {

	public static void main(String[] args) {
		
		CustomStringTR metin= new CustomStringTR("metin");
		
		
		System.out.println(metin.uzunluk());
		System.out.println(metin.buyukHarfYap());
		System.out.println(metin.kucukHarfYap());
		System.out.println(metin.karakter(4));
		System.out.println(metin.harf("e"));
		System.out.println(metin.sonHarf());
		System.out.println(metin.bitisKontrol("a"));
		System.out.println(metin.baslangicKontrol("a"));
		
		
		}

}
